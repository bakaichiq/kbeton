# Core
ENV=dev
TZ=Asia/Bishkek
LOG_LEVEL=INFO

# DB
POSTGRES_DB=kbeton
POSTGRES_USER=kbeton
POSTGRES_PASSWORD=CHANGE_ME_DB_PASSWORD
DATABASE_URL=postgresql+psycopg2://kbeton:CHANGE_ME_DB_PASSWORD@postgres:5432/kbeton

# Redis/Celery
REDIS_URL=redis://redis:6379/0
CELERY_BROKER_URL=redis://redis:6379/1
CELERY_RESULT_BACKEND=redis://redis:6379/2
BOT_FSM_STORAGE=redis
BOT_FSM_REDIS_URL=redis://redis:6379/3

# Telegram
TELEGRAM_BOT_TOKEN=
TELEGRAM_DEFAULT_CHAT_ID=

# API Security
API_AUTH_ENABLED=true
API_TOKEN=CHANGE_ME_LONG_RANDOM_API_TOKEN

# S3/MinIO
S3_ENDPOINT_URL=http://minio:9000
S3_ACCESS_KEY_ID=CHANGE_ME_MINIO_ACCESS_KEY
S3_SECRET_ACCESS_KEY=CHANGE_ME_MINIO_SECRET_KEY
S3_BUCKET=kbeton
S3_REGION=us-east-1
